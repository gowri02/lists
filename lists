#printing th positive elements in the list
input_list=[] #input list

value= int(input("enter the range: ")) #entering the range
for i in range(value):
    val=int(input("enter the value: "))
    input_list.append(val)#adding elements to the list
print(f'input list: {input_list}')   
for i in input_list:
    if i<0:
        input_list.remove(i) #if the number is negative, remove the number from list
print(f'output list:{input_list}') 
